    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-relay-plugin": "0.9.3",
    "classnames": "2.2.5",
    "express": "4.14.0",
    "express-graphql": "0.6.4",
    "graphql": "0.9.3",
    "graphql-relay": "0.5.1",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-relay": "dev",
    "relay-compiler": "dev",
    "relay-runtime": "dev",
    "webpack": "1.13.2",
    "webpack-dev-server": "1.15.1"
